//console.log("@last_user_event_id = <%= @last_user_event_id %>");
//console.log("@ues.count = <%= @ues.count %>");

<% if @ues && @ues.count > 0 %>
  <% @ues.each do |ue| %>
//    console.log("ue.id = <%= ue.id %>");
    add_msg(<%= ue.id %>, "<%= ue.mid %>", "<%= j(ue.user_name) %>", "<%= j(ue.user_url) %>", "<%= j(ue.user_status) %>","<%= j(ue.msg) %>", "<%= ue.created_at %>", "<%= ue.created_at.to_s(:human) %>")
  <% end %>
<% end %>

$("#last_user_event_id").val(<%= @last_user_event_id %>);
